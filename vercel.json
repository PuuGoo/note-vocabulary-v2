{
  "version": 2,
  "rewrites": [
    {
      "source": "/css/(.*)",
      "destination": "/public/css/$1"
    },
    {
      "source": "/js/(.*)",
      "destination": "/public/js/$1"
    },
    {
      "source": "/(.*\\.(html|ico|png|jpg|jpeg|svg|gif))",
      "destination": "/public/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/src/server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
